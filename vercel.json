{
  "version": 2,
  "buildCommand": "CLIENT_TYPE=production npm run build:production",
  "builds": [
    {
      "src": "build/index-http.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["build/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/build/index-http.js"
    },
    {
      "src": "/sse",
      "dest": "/build/index-http.js"
    },
    {
      "src": "/mcp",
      "dest": "/build/index-http.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index-http.js"
    }
  ],
  "env": {
    "CLIENT_TYPE": "production"
  }
}
